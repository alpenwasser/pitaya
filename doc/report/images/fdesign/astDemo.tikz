            width=0.53\textwidth,
            at = {(0.47\textwidth,-130mm)},
            ylabel={},
            y unit={},
            yticklabel pos=right,
            yticklabel style={font=\footnotesize},
            %xtick={0,0.5,0.6,1},
            %xticklabels={0,0.5,0.6,$f_\mathrm{s1}/2$},
        ]
        \addplot[thick,q1,-] table[x=W, y=abs(H)] \freqzTableAstB;
