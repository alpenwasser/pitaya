\pgfplotstableread[col sep=comma]{\freqzFileAstA}\freqzTableAstA
\pgfplotstableread[col sep=comma]{\freqzFileAstB}\freqzTableAstB
\pgfplotstableread[col sep=comma]{\freqzFileAstC}\freqzTableAstC
